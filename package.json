{
  "name": "vite-css-modules",
  "version": "0.0.1",
  "description": "Prototype for using CSS Modules in Vite",
  "author": "Curtis Dulmage",
  "license": "ISC",
  "keywords": [
    "Vite",
    "CSS",
    "Modules"
  ],
  "homepage": "https://github.com/beefchimi/vite-css-modules#readme",
  "bugs": {
    "url": "https://github.com/beefchimi/vite-css-modules/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beefchimi/vite-css-modules.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "extends @beefchimi/browserslist-config"
  ],
  "prettier": "@beefchimi/prettier-config",
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "build": "npm run clean && tsc && vite build",
    "clean": "rm -rf coverage && rm -rf dist",
    "start": "vite",
    "preview": "vite preview",
    "nuke": "npm run clean && rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force"
  },
  "devDependencies": {
    "@beefchimi/browserslist-config": "^0.0.9",
    "@beefchimi/eslint-plugin": "^0.0.9",
    "@beefchimi/prettier-config": "^0.0.9",
    "@beefchimi/typescript-config": "^0.0.9",
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.2.0",
    "vite": "^2.8.4",
    "vite-plugin-dts": "^0.9.9"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
